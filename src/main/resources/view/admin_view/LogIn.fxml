<AnchorPane blendMode="DARKEN" prefHeight="568.0" prefWidth="1084.0" stylesheets="@../../css/admin_css/style_login.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Admin.LogIn">
