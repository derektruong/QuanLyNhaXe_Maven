            </ImageView>
            <Button id="blue" fx:id="btn_login" layoutX="67.0" layoutY="435.0" mnemonicParsing="false" onMouseClicked="#btn_login_clicked" styleClass="blue" stylesheets="@../../css/admin_css/style_login.css" text="Log in" textFill="WHITE">
